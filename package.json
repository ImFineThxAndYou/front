    "lucide-react": "^0.539.0",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^13.5.0",
    "react-is": "^19.1.1",
    "recharts": "^3.0.2",
    "sockjs-client": "^1.6.1",
